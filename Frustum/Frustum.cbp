<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Frusutm" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output=".\frustum" prefix_auto="1" extension_auto="1" />
				<Option object_output="objd\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="sky.png" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output=".\frustum" prefix_auto="1" extension_auto="1" />
				<Option object_output="objs\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add directory=".\include" />
			<Add directory="..\external\includes" />
		</Compiler>
		<Linker>
			<Add library=".\SDL.dll" />
			<Add library=".\SITGL.dll" />
			<Add library="opengl32" />
			<Add library="user32" />
		</Linker>
		<Unit filename="frustum.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="utils.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
